#%RAML 1.0
title: Headers Test Case

/datetime2616:
  get:
    headers:
      X-ModifiedSince:
        type: datetime
        format: rfc2616
  post:
    headers:
      X-MaxRows:
        type: integer
        required: false
      defaultHeaderValue:
        type: string
        default: "hola-header"
        required: false

/test-array-header:
  get:
    headers:
      array-header:
        required: false
        type: array
        items:
          type: object
          properties:
            type: string
            value: string

/multiplicity:
  get:
    responses:
      200:
        body:
          application/json:
  post:
    body:
      application/json: