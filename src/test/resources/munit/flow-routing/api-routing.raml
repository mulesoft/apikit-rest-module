#%RAML 1.0
---
title: API for test routing!

types:
  ApplyCreditMemoType:
    type: object
    properties:
      effectiveDate:
        description: |
          The date when the credit memo is applied.
        type: date-only
        required: false

/apply-with-charset:
  put:
    responses:
      200:
        body:
            application/json; charset=utf-8:
              type: ApplyCreditMemoType
    body:
      application/json; charset=utf-8:
        type: ApplyCreditMemoType

/{resourceType}:{id}:
  get:
    responses:
      200:
        body:
          application/json: !!null

/uri-param:
  /{uriID}/test:
    get:
      responses:
        200:
          body:
            application/json:
              type: ApplyCreditMemoType

/customers/{customerID}/applications/{applicationID}:
  post:
    body:
      application/json:
        type: ApplyCreditMemoType

/mapping:
  get:
    responses:
      200:
        body:
          application/json:
            type: ApplyCreditMemoType

/notImplemented:
  get:
    responses:
      200:

/implementOnlyOneMediaType:
  put:
    body:
      application/json:
      application/xml: